<h1>Users#edit</h1>
   <%= form_for(@user) do |form| %>
      <%= form.label :username %> <br/> 
      <%= form.text_field :name %> <br/> 
      <%= form.label :first_name %> <br/> 
      <%= form.text_field :first_name %> <br/> 
      <%= form.label :last_name %> <br/> 
      <%= form.text_field :last_name %> <br/> 
      <%= form.label :email %> <br/> 
      <%= form.email_field :email %> <br/> 
      <%= form.label :phone %> <br/> 
      <%= form.telephone_field :phone %> <br/> <br/>
      <%# <%= form.label :password %>  
      <%# <%= form.password_field :password %> 
      <%= form.submit "Submit", class: "btn btn-jindigo" %><br/> <br/> 
    <% end %>

<%= link_to 'Show', @user,  class: "btn btn-jindigo" %>
<%= link_to 'Back', users_path,  class: "btn btn-jindigo" %>
